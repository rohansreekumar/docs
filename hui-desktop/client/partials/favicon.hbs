<link rel="icon" href="/{{path.temp_favicon}}/favicon.ico" type="image/x-icon"/>
{{#favicons.apple-touch-icon}}
    <link rel="apple-touch-icon" sizes="{{sizes}}" href="/{{../path.temp_favicon}}/apple-touch-icon-{{sizes}}.png">
{{/favicons.apple-touch-icon}}
{{#favicons.android-icon}}
    <link rel="icon" type="image/png" href="/{{../path.temp_favicon}}/favicon-{{sizes}}.png" sizes={{sizes}}>
{{/favicons.android-icon}}
{{#favicons.chrome-icon}}
    <link rel="icon" type="image/png" href="/{{../path.temp_favicon}}/android-chrome-{{sizes}}.png" sizes="{{sizes}}">
{{/favicons.chrome-icon}}
<link rel="manifest" href="/{{../path.temp_favicon}}/android-chrome-manifest.json">
<meta name="msapplication-TileColor" content="#eb8900">
{{#favicons.ms-icon}}
    <meta name="msapplication-TileImage" content="/{{../path.temp_favicon}}/mstile-{{sizes}}.png">
{{/favicons.ms-icon}}
<meta name="theme-color" content="#ffffff">

<!doctype html>
<html class="no-js" lang="en">

<head>
    <meta charset="utf-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
	<meta name="fragment" content="!">
    <title data-ng-bind="pageMeta.$current.title">{{title}}</title>
    <meta name="description" content="\{{pageMeta.$current.desc}}" />
    <meta ng-repeat="meta in pageMeta.$current.head_meta" name="\{{meta.name}}" value="\{{meta.value}}" property="\{{meta.property}}" content="\{{meta.content}}" />
    <meta ng-if="pageMeta.$current.og_title" property="og:title" content="\{{pageMeta.$current.og_title}}" />
    <meta ng-if="pageMeta.$current.og_desc" property="og:description" content="\{{pageMeta.$current.og_desc}}" />
    <meta ng-if="pageMeta.$current.og_type" property="og:type" content="\{{pageMeta.$current.og_type}}" />
    <meta ng-if="pageMeta.$current.og_site_name" property="og:site_name" content="\{{pageMeta.$current.og_site_name}}" />
    <link ng-if="pageMeta.$current.mobile_url" rel="alternate" ng-href="\{{pageMeta.$current.mobile_url}}"/>
    <link ng-if="pageMeta.$current.canonical" rel="canonical" ng-href="\{{pageMeta.$current.canonical}}"/>
    <meta ng-if="pageProps.$current.page.redirect" name="prerender-status-code" content="\{{pageProps.$current.goto.code || 301}}">
    <meta ng-if="pageProps.$current.page.redirect" name="prerender-header" content="Location: \{{$location.protocol() + '://' + $location.host() + pageProps.$current.page.uri}}" >
    <base href="/"/>

    <!--[if IE 9]>
    <script src="//cdn.rawgit.com/jpillora/xdomain/0.7.4/dist/xdomain.min.js" slave="http://{{api_public_host}}/proxy.html"></script>
    <![endif]-->

    {{{newRelic}}}

    {{> favicon}}

    {{#fonts}}
        <link rel="stylesheet" href="/{{path}}/{{css}}"/>
    {{/fonts}}
    <link rel="stylesheet" href="/css/app.css?v=20160926.2"/>
    <link rel="stylesheet" href="/css/main.css?v=20160926.2"/>
    <script>
        window.app_config = ['{{api_public_host}}', '{{temp_key}}', '{{environment.mask}}', '{{legacy_host}}', '{{log_host}}'];
       {{#initial_props}}
           window.initial_props = {{initial_props}};
       {{/initial_props}}
   </script>
    <script src="/js/vendor/modernizr/modernizr.js"></script>
</head>

<body ng-class="'page-' + $root.pageProps.$current.page.type">
    <!-- ****************** !-->
    <!-- If IE 9 or earlier !-->
    <!-- ****************** !-->
    <!-- [if lte IE 9]>
    <div class="ie-update">
        <nav class="global-nav-header">
            <div class="logo"><a href="/" title="Homes.com">Homes.com</a></div>
        </nav>
        <div class="ie-message-container">
            <div class="ie-message-text">
                <p>Uh oh...</p>
                <p>It seems you are using an outdated browser.</p>
                <p>Please <a href="http://outdatedbrowser.com/" target="_blank">upgrade your browser</a> to improve your experience.</p>
            </div>
        </div>
    </div>
    <![endif]-->

    <header class="frame-header" id="header" ui-view="header">
    </header>

    <section id="sub-header" ui-view="sub-header">
    </section>

    <section id="main" ui-view="content">
    </section>

    <footer  ui-view="footer">
    </footer>

    {{! Scripts}}
    <script src="/js/main.js?v=20160919.2"></script>
    {{#local}}
        <script src="//localhost:{{server.livereload}}/livereload.js"></script>
    {{/local}}
</body>

</html>
